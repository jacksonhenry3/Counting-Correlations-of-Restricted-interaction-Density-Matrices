Cell[BoxData[
 RowBox[{
  RowBox[{"allSetsOfSwaps", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "allSwaps", "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
       RowBox[{"if", " ", "E"}], "=", 
       RowBox[{
       "2", " ", "the", " ", "number", " ", "of", " ", "swaps", " ", 
        "consider", " ", "doesnt", " ", "need", " ", "to", " ", "be", " ", 
        "more", " ", "than", " ", "2"}]}], ",", " ", 
      RowBox[{
      "in", " ", "general", " ", "2", " ", "should", " ", "be", " ", 
       "replaced", " ", "with", " ", "n"}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"allSwaps", " ", "=", " ", 
      RowBox[{"Permutations", "[", 
       RowBox[{
        RowBox[{"PossibleSwaps", "[", "n", "]"}], ",", "2"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "remove", " ", "sets", " ", "of", " ", "swaps", " ", "if", " ", "any", 
       " ", "two", " ", "of", " ", "the", " ", "swaps", " ", "overlap"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"allSwaps", " ", "=", " ", 
      RowBox[{"Select", "[", 
       RowBox[{"allSwaps", ",", 
        RowBox[{
         RowBox[{"DuplicateFreeQ", "[", 
          RowBox[{"Flatten", "[", "#", "]"}], "]"}], "&"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "remove", " ", "duplicate", " ", "sets", " ", "of", " ", "swaps"}], 
      "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"sort", " ", "is", " ", "used", " ", "so", " ", "that", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", "2"}], "}"}], 
