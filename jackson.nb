Cell[BoxData[
 RowBox[{
  RowBox[{"FullAdjacencyMatrixEnergyBasis", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "protoState", ",", "allStates", ",", "numStates", ",", "edges", ",", 
      "stateAdjacencyMatrix", ",", "stateNum", ",", "correlationNum", ",", 
      "numbers", ",", "index"}], "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"stateAdjacencyMatrix", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        SuperscriptBox["2", "n"], ",", 
        SuperscriptBox["2", "n"]}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"allStates", " ", "=", " ", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"protoState", " ", "=", " ", 
         RowBox[{"Join", "[", 
          RowBox[{
           RowBox[{"Table", "[", 
            RowBox[{"0", ",", 
             RowBox[{"n", "-", "e"}]}], "]"}], ",", 
           RowBox[{"Table", "[", 
            RowBox[{"1", ",", "e"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"allStates", " ", "=", " ", 
         RowBox[{"Union", "[", 
          RowBox[{"allStates", ",", 
           RowBox[{"Permutations", "[", "protoState", "]"}]}], "]"}]}], ";"}],
        "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"e", ",", "0", ",", "n"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"numStates", " ", "=", " ", 
      RowBox[{"Length", "[", "allStates", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"numbers", " ", "=", " ", 
      RowBox[{"Sort", "[", 
