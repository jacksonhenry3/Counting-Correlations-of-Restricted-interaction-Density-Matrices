Cell[BoxData[
 RowBox[{
  RowBox[{"allSetsOfSwaps", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "allSwaps", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"allSwaps", " ", "=", " ", 
      RowBox[{"Permutations", "[", 
       RowBox[{
        RowBox[{"PossibleSwaps", "[", "n", "]"}], ",", "n"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "remove", " ", "sets", " ", "of", " ", "swaps", " ", "if", " ", "any", 
       " ", "two", " ", "of", " ", "the", " ", "swaps", " ", "overlap"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"allSwaps", " ", "=", " ", 
      RowBox[{"Select", "[", 
       RowBox[{"allSwaps", ",", 
        RowBox[{
         RowBox[{"DuplicateFreeQ", "[", 
          RowBox[{"Flatten", "[", "#", "]"}], "]"}], "&"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "remove", " ", "duplicate", " ", "sets", " ", "of", " ", "swaps"}], 
      "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"sort", " ", "is", " ", "used", " ", "so", " ", "that", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", ",", "2"}], "}"}], 
         RowBox[{"{", 
          RowBox[{"3", ",", "4"}], "}"}]}], "}"}], " ", "matches", " ", 
       "with", " ", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"3", ",", "4"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"1", ",", "2"}], "}"}]}], "}"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"allSwaps", " ", "=", " ", 
      RowBox[{"DeleteDuplicates", "[", 
       RowBox[{"allSwaps", ",", 
        RowBox[{
         RowBox[{
          RowBox[{"Sort", "[", "#1", "]"}], "==", 
          RowBox[{"Sort", "[", "#2", "]"}]}], "&"}]}], "]"}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
