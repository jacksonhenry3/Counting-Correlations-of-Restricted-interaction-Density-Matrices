Cell["\<\
This function give the adjacency matrix for a given number of qbits and \
energy level. It is different than running AdjacencyMatrix[StateGraph[n,e]] \
because it enforces canonical ordering.\
\>", "Text",
 CellChangeTimes->{{3.885064521248392*^9, 
  3.8850646011800795`*^9}},ExpressionUUID->"3782f312-1c6a-44f3-a3ed-\
9a187d2e6e67"],

Cell[BoxData[
 RowBox[{
  RowBox[{"AdjacencyMatrixInEnergySubspaceforEnergy", "[", 
   RowBox[{"n_", ",", "e_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "protoState", ",", "allStates", ",", "numStates", ",", "edges", ",", 
      "stateAdjacencyMatrix", ",", "stateNum", ",", "correlationNum", ",", 
      "numbers"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Assert", "[", 
      RowBox[{"n", ">", "e"}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"protoState", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"i", "<=", "e"}], ",", "1", ",", "0"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"allStates", " ", "=", " ", 
      RowBox[{"Permutations", "[", "protoState", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"numStates", " ", "=", " ", 
      RowBox[{"Length", "[", "allStates", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"stateAdjacencyMatrix", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", "numStates", ",", "numStates"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"numbers", " ", "=", " ", 
      RowBox[{"Sort", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"FromDigits", "[", 
          RowBox[{"state", ",", "2"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"state", ",", "allStates"}], "}"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"stateNum", " ", "=", 
         RowBox[{
          RowBox[{"Position", "[", 
           RowBox[{"numbers", ",", 
            RowBox[{"FromDigits", "[", 
             RowBox[{"state", ",", "2"}], "]"}]}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"correlationNum", " ", "=", " ", 
         RowBox[{
          RowBox[{"Position", "[", 
           RowBox[{"numbers", ",", 
            RowBox[{"FromDigits", "[", 
             RowBox[{"correlation", ",", "2"}], "]"}]}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"stateAdjacencyMatrix", "[", 
          RowBox[{"[", 
           RowBox[{"stateNum", ",", "correlationNum"}], "]"}], "]"}], " ", 
         "=", " ", "1"}], ";"}], "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"state", ",", "allStates"}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"correlation", ",", 
         RowBox[{"PossibleCorrelation", "[", "state", "]"}]}], "}"}]}], "]"}],
      ";", "\[IndentingNewLine]", "stateAdjacencyMatrix"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
