      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"FromDigits", "[", 
         RowBox[{"num", ",", "2"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"num", ",", "numbers"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"stateNum", " ", "=", 
         RowBox[{
          RowBox[{"Position", "[", 
