          RowBox[{"1", ",", "2"}], "}"}]}], "}"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"allSwaps", " ", "=", " ", 
      RowBox[{"DeleteDuplicates", "[", 
       RowBox[{"allSwaps", ",", 
        RowBox[{
         RowBox[{
          RowBox[{"Sort", "[", "#1", "]"}], "==", 
